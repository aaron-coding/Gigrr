<!DOCTYPE html>
<html>
<head>
  <title>Gigrr</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  
  <% if logged_in? %>
    <%= button_to "Log out", session_url, method: "delete" %>
  <% else %>
    <%= render "login" %>
  <% end %>
  
<%= flash[:errors] %>

<%= yield %>

</body>
</html>
