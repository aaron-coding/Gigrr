<!DOCTYPE html>
<html>
<head>
  <title>Buy Five</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag 'favicon.ico' %>
</head>
<body>
  <%= render 'layouts/navbar' %>  
  <div id="main-wrapper">

    <% if flash[:errors] %>
      <%= render 'layouts/errors' %>
    <% end %>

    <%= yield %>
  </div>    
  <%= render 'layouts/footer' %>
</body>
</html>
